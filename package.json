{
  "name": "octopus-rc-controls",
  "version": "1.0.0",
  "description": "octopus reactjs controls",
  "scripts": {
    "format-js": "prettier --single-quote --trailing-comma es5 --print-width=140 \"src/**/*.js\"",
    "format-style": "prettier --single-quote --trailing-comma es5 --print-width=140 \"src/**/*.scss\"",
    "format": "npm run format-js & npm run format-style",
    "clean": "gulp clean",
    "copy:meta": "gulp copy",
    "build:jsx": "cross-env BABEL_OUTPUT=commonjs babel src/ --out-dir dist/ --ignore **/__tests__,**/__mocks__",
    "build:sass": "node-sass src/shared/styles -o dist/styles",
    "build": "npm run clean && npm run build:jsx && npm run build:sass && npm run copy:meta",
    "build:fmt": "npm run format & npm run build"
  },
  "main": "dist/index.js",
  "author": "Duy Luong Phung",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krakenui/octopus-rc-controls.git"
  },
  "keywords": [
    "reactjs",
    "controls",
    "control",
    "navi",
    "controls",
    "navi-controls",
    "navitech"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "4.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.4.0",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "html-to-draftjs": "^1.4.0",
    "react-draft-wysiwyg": "^1.13.2",
    "antd": "^3.18.1",
    "react-slick": "^0.24.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
